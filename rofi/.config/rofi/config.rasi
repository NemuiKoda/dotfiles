configuration{
    modi: "window,drun,combi";
    icon-theme: "Oranchelo";
    show-icons: true;
    terminal: "ghostty";
    drun-display-format: "{icon} {name}";
    location: 0;
    disable-history: false;
    hide-scrollbar: true;
    display-combi: " ALL ";
    display-run: " RUN ";
    display-window: " WINDOW ";
    display-drun: " DRUN ";
    sidebar-mode: true;
    height: 30%;
    width: 50%;
}

@theme "rose-pine"
